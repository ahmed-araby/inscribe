<!DOCTYPE html>
<html>
<head>
    <!-- Standard Meta -->
    <meta charset="utf-8"/>
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1"/>
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0">

    <link rel="stylesheet" type="text/css"
          href="{{ asset('bundles/app/App/node_modules/semantic-ui/dist/semantic.min.css') }}">
    <link rel="stylesheet" type="text/css" href="{{ asset('bundles/app/css/app.css') }}">

    <!-- Site Properties -->
    <title>Inscribe</title>
</head>
<body>

<div class="ui one column centered grid container">
    {% for type, messages in app.session.flashBag.all %}
        {% for message in messages %}
            <div class="ui message {{ type }}">
                {{ message|trans({}, 'FOSUserBundle') }}
            </div>
        {% endfor %}
    {% endfor %}

    {% block  content %}
    {% endblock %}
</div>

<script src="{{ asset('bundles/app/App/node_modules/jquery/dist/jquery.min.js') }}"></script>
<script src="{{ asset('bundles/app/App/node_modules/semantic-ui/dist/semantic.min.js') }}"></script>
<script src="{{ asset('bundles/app/App/node_modules/jquery.runner/build/jquery.runner-min.js') }}"></script>
<script src="{{ asset('bundles/app/js/app.js') }}"></script>
{% block footerscripts %}
{% endblock %}
</body>
</html>